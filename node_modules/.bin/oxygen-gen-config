#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node" --no-warnings "$basedir/../@shopify/mini-oxygen/dist/gen-config.js" "$@"
else 
  exec node --no-warnings "$basedir/../@shopify/mini-oxygen/dist/gen-config.js" "$@"
fi
